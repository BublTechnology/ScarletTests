{
        "preset": "jquery",
                "plugins": [
                        "esformatter-quotes",
                        "esformatter-semicolons",
                        "esformatter-braces",
                        "esformatter-dot-notation"
                        ],
                "quotes": {
                        "type": "single",
                        "avoidEscape": false
                },
                "indent": {
                        "value": "  "  // 2 spaces
                },
                "whiteSpace": {
                        "before": {
                                "ArgumentList": 0,
                                "ArgumentListObjectExpression": 0,
                                "ArgumentListFunctionExpression": 0,
                                "ArgumentListArrayExpression": 0
                        },
                        "after": {
                                "ArgumentList": 0,
                                "ArgumentListObjectExpression": 0,
                                "ArgumentListFunctionExpression": 0,
                                "ArgumentListArrayExpression": 0,
                                "FunctionReservedWord": 1,
                                "FunctionExpressionClosingBrace": 0
                        }
                }
}
